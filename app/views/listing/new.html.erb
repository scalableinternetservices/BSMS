<div id="form_wrapper">
  <%= form_for @listing, url: {action: "create"} do |f| %>
    <%= f.label :location, "location" %>
    <%= f.text_area :location %>
    <%= f.label :price, "price" %>
    <%= f.text_area :price %>
    <%= f.label :duration, "duration" %>
    <%= f.text_area :duration %>
    <%= f.label :amenities, "amenities" %>
    <%= f.text_area :amenities %>
    <%= f.label :housing_type, "housing_type" %>
    <%= f.text_area :housing_type %>
    <%= f.label :description, "description" %>
    <%= f.text_area :description %>
    <%= f.submit "Submit" %>
   <% end %>
 </div>