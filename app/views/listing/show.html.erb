<div class='centered'>
  <h3>My Listings</h3>
  <%= link_to 'Create listing', listing_new_path, class: 'btn btn-primary'%>
  <% if @listing %>
    <% @listing.each_with_index do |data, i| %>
      <h5>Listing #<%=i+1%></h5>
    <table>
      <tbody>
        <tr>
          <th scope="row">Location</th>
          <td><%= data.location %></td>
        </tr>
        <tr>
          <th scope="row">Duration</th>
          <td><%= data.duration %></td>
        </tr>
        <tr>
          <th scope="row">Housing Type</th>
          <td><%= data.housing_type %></td>
        </tr>
        <tr>
          <th scope="row">Number of Bedrooms</th>
          <td><%= data.bedrooms %></td>
        </tr>
        <tr>
          <th scope="row">Number of Bathrooms</th>
          <td><%= data.bathrooms %></td>
        </tr>
        <tr>
          <th scope="row">Onsite Gym</th>
          <td><%= data.gym %></td>
        </tr>
        <tr>
          <th scope="row">Pet Friendly</th>
          <td><%= data.pet_friendly %></td>
        </tr>
        <tr>
          <th scope="row">In-Unit Air Conditioning</th>
          <td><%= data.ac %></td>
        </tr>
        <tr>
          <th scope="row">In-Unit Heater</th>
          <td><%= data.heater %></td>
        </tr>
        <tr>
          <th scope="row">Wi-Fi Provided</th>
          <td><%= data.wifi %></td>
        </tr>
        <tr>
          <th scope="row">Washer and Dryer Provided</th>
          <td><%= data.wash_and_dry %></td>
        </tr>
        <tr>
          <th scope="row">Yard Space</th>
          <td><%= data.yard %></td>
        </tr>
        <tr>
          <th scope="row">Near Public Transportation</th>
          <td><%= data.public_transportation %></td>
        </tr>
        <tr>
          <th scope="row">Number of Parking Spaces Available</th>
          <td><%= data.parking %></td>
        </tr>
        <tr>
          <th scope="row">Price Cap (Highest Price)</th>
          <td><%= data.price %></td>
        </tr>
        <tr>
          <th scope="row">Additional Information</th>
          <td><%= data.description %></td>
        </tr>
      </tbody>
    </table>
    <div>
      <%= link_to 'Edit listing', listing_edit_path, class: 'btn btn-primary'%>
      <%= link_to 'Delete listing', listing_destroy_path, class: 'btn btn-primary'%>
    </div>
    <% end %>
  <% else %>
    <h3>No listings set</h3>
    <div>
      <%= link_to 'Create listing', listing_new_path, class: 'btn btn-primary'%>
    </div>
  <% end %>
</div>
