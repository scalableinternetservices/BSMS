<div id='bg'>
  <div class='centered'>
    <h1>Wanderlease</h1>
    <% unless !current_user %>
      <p>Welcome, <%= current_user[:email]%></p>
    <% end %>
  </div>
  <% unless !current_user %>
  
  <div class='centered'>
    <h3>Listings</h3>
    <table>
    	<tr>
    		<th> location </th>
    		<th> price </th>
    		<th> duration </th>
    		<th> amenities </th>
    		<th> housing type </th>
    		<th> description </th>
    	</tr>
    
      <% @listings.each do |data| %>
    	  <tr>
    			<td><%= data.location %></td>
    			<td><%= data.price %></td>
    			<td><%= data.duration %></td>
    			<td><%= data.amenities %></td>
    			<td><%= data.housing_type %></td>
    			<td><%= data.description %></td>
    	  </tr>
      <% end %>
      
    </table>
  </div>

  <% end %>
</div>
